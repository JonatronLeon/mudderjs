{
  "name": "mudder",
  "version": "0.0.1",
  "description": "Find lexicographical midpoints between strings",
  "main": "dist/mudder.js",
  "module": "index",
  "jsnext:main": "index",
  "scripts": {
    "pretest": "rm -fr dist && mkdir -p dist && rollup -c",
    "test": "tape 'test/**/*-test.js'",
    "build": "npm run test && uglifyjs dist/mudder.js -c -m -o dist/mudder.min.js"
  },
  "keywords": [
    "string",
    "mean",
    "lexicographical",
    "distance",
    "number",
    "radix",
    "base"
  ],
  "author": "Ahmed Fasih",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/fasiha/mudderjs.git"
  },
  "dependencies": {
    "big.js": "^3.1.3"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.2.0",
    "object.entries": "^1.0.3",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-browserify-transform": "^0.1.0",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "source-map-support": "^0.4.2",
    "tape": "^4.6.0",
    "uglify-js": "^2.7.3"
  }
}
